<table class="table">
  <tr>
    <th>Applicant</th>
    <th>Address</th>
    <th>ANC</th>
  </tr>
  <% @zoning_cases.each do |zoning_case| %>
    <tr>
      <td>
        <%= link_to zoning_case.applicant, {
          controller: :zoning_cases,
          action: :show,
          id: zoning_case.id
          } %>
      </td>
      <td><%= zoning_case.address %></td>
      <td><%= zoning_case.anc.name if zoning_case.anc %></td>
    </tr>
  <% end %>
</table>

<%= will_paginate @zoning_cases %>
